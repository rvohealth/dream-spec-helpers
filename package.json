{
  "type": "module",
  "name": "@rvoh/dream-spec-helpers",
  "version": "2.0.0",
  "description": "dream orm spec helpers",
  "author": "RVO Health",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rvohealth/dream-spec-helpers.git"
  },
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/src/index.js",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/src/index.js",
      "import": "./dist/esm/src/index.js",
      "types": "./dist/types/src/index.d.ts"
    }
  },
  "files": [
    "tsconfig.json",
    "src/**/*",
    "dist/**/*"
  ],
  "scripts": {
    "build": "echo \"building dream-spec-helpers...\" && rm -rf dist && npx tsc -p ./tsconfig.cjs.build.json && npx tsc -p ./tsconfig.esm.build.json",
    "format": "pnpm run prettier . --write",
    "prepack": "yarn build",
    "lint": "pnpm eslint --no-warn-ignored \"src/**/*.ts\" && pnpm prettier . --check"
  },
  "peerDependencies": {
    "@types/node": "*",
    "@types/pg": "*",
    "pg": "*"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^22.5.1",
    "@types/pg": "^8.11.8",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^9.39.1",
    "pg": "^8.12.0",
    "prettier": "^3.7.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.48.1",
    "vitest": "^4.0.10"
  },
  "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a",
  "dependencies": {
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "pluralize-esm": "^9.0.5"
  }
}
